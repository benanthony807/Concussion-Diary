        <activity android:name="com.example.contactdiary.ui.entryscreens.questionscreens.Question3Screen" />
        <activity android:name="com.example.contactdiary.ui.entryscreens.questionscreens.Question4Screen" />
        <activity android:name="com.example.contactdiary.ui.entryscreens.questionscreens.Question5Screen" />
        <activity android:name="com.example.contactdiary.ui.entryscreens.questionscreens.Question6Screen" />
        <activity android:name="com.example.contactdiary.ui.entryscreens.questionscreens.Question7Screen" />
