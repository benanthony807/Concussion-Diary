        <activity android:name="com.example.contactdiary.ui.entryscreens.questionscreens.Question10Screen" />
        <activity android:name="com.example.contactdiary.ui.entryscreens.questionscreens.Question11Screen" />
        <activity android:name="com.example.contactdiary.ui.entryscreens.questionscreens.Question12Screen" />
