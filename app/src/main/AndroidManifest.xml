        <activity android:name="com.example.contactdiary.ViewRecord" />


        <activity android:name="com.example.contactdiary.ui.entryscreens.NewEntryMainScreen" />
        <activity android:name="com.example.contactdiary.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

